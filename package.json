{
    "name": "ng-numeric-keyboard",
    "version": "1.0.0",
    "description": "Number keyboard for mobile browsers For Angular",
    "author": "m310851010",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "test:headless": "ng test ng-numeric-keyboard --watch=false --browsers=ChromeHeadless",
        "test:ng-numeric-keyboard": "ng test ng-numeric-keyboard",
        "prepublish": "ng build --project=ng-numeric-keyboard",
        "publish": "cd dist/ng-numeric-keyboard && npm publish --access=public",
        "pack:ng-numeric-keyboard": "cd dist/ng-numeric-keyboard && npm pack",
        "package": "ng build ng-numeric-keyboard && npm run pack:ng-numeric-keyboard",
        "build:dev": "ng build --project=ng-numeric-keyboard --watch"
    },
    "private": false,
    "dependencies": {
        "@angular/animations": "^14.0.0",
        "@angular/common": "^14.0.0",
        "@angular/compiler": "^14.0.0",
        "@angular/core": "^14.0.0",
        "@angular/forms": "^14.0.0",
        "@angular/platform-browser": "^14.0.0",
        "@angular/platform-browser-dynamic": "^14.0.0",
        "@angular/router": "^14.0.0",
        "rxjs": "~7.5.6",
        "tslib": "~2.4.0",
        "zone.js": "~0.11.8"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^14.0.0",
        "@angular/cli": "^14.0.0",
        "@angular/compiler-cli": "^14.0.0",
        "@types/jasmine": "~4.0.0",
        "@types/node": "~14.0.0",
        "@angular-eslint/builder": "~14.0.2",
        "@angular-eslint/eslint-plugin": "~14.0.2",
        "@angular-eslint/eslint-plugin-template": "~14.0.2",
        "@angular-eslint/schematics": "~14.0.2",
        "@angular-eslint/template-parser": "~14.0.2",
        "@typescript-eslint/eslint-plugin": "~5.30.7",
        "@typescript-eslint/parser": "~5.30.7",
        "jasmine-core": "~4.1.0",
        "karma": "~6.3.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.0.0",
        "karma-jasmine-html-reporter": "~1.7.0",
        "ng-packagr": "~14.0.0",
        "typescript": "~4.7.2",
        "eslint": "8.20.0",
        "eslint-config-prettier": "~8.5.0",
        "eslint-plugin-import": "2.26.0",
        "eslint-plugin-jsdoc": "39.3.3",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-prettier": "~4.2.1",
        "prettier": "~2.7.1",
        "prettier-eslint": "~15.0.1"
